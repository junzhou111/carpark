<view class="pages"><view class="page-header"><view class="header-title">停车详情</view><block wx:if="{{item.io}}"><view class="header-item"><view class="item-left">进出类型</view><view class="item-right">{{item.io==0?'进':item.io==0?'出':'暂无数据'}}</view></view></block><block wx:if="{{item.location}}"><view class="header-item"><view class="item-left">位置id</view><view class="item-right">{{item.location}}</view></view></block><block wx:if="{{item.time}}"><view class="header-item"><view class="item-left">入场/出场时间</view><view class="item-right">{{item.time}}</view></view></block><block wx:if="{{item.location_type}}"><view class="header-item"><view class="item-left">位置类型</view><view class="item-right">{{item.location_type==0?'车场':item.location_type==1?'区域':'暂无数据'}}</view></view></block><block wx:if="{{item.person_id}}"><view class="header-item"><view class="item-left">用户名称</view><view class="item-right">{{item.person_id}}</view></view></block><block wx:if="{{item.carscan}}"><view class="header-item"><view class="item-left">车牌</view><view class="item-right">{{item.carscan}}</view></view></block><block wx:if="{{item.cartype}}"><view class="header-item"><view class="item-left">车型</view><view class="item-right">{{item.cartype}}</view></view></block><block wx:if="{{item.carcolor}}"><view class="header-item"><view class="item-left">车身颜色</view><view class="item-right">{{item.carcolor}}</view></view></block><block wx:if="{{item.order_number}}"><view class="header-item"><view class="item-left">支付单号</view><view class="item-right small">{{item.order_number}}</view></view></block><block wx:if="{{item.freetime}}"><view class="header-item"><view class="item-left">免费时常</view><view class="item-right">{{item.freetime}}</view></view></block><block wx:if="{{item.triggertype}}"><view class="header-item"><view class="item-left">触发方式</view><view class="item-right">{{item.triggertype}}</view></view></block><block wx:if="{{item.release}}"><view class="header-item"><view class="item-left">是否放行</view><view class="item-right">{{item.release}}</view></view></block><block wx:if="{{item.isout}}"><view class="header-item"><view class="item-left">是否已出场</view><view class="item-right">{{item.isout}}</view></view></block><block wx:if="{{item.feetype}}"><view class="header-item"><view class="item-left">计费形式</view><view class="item-right">{{''+(item.feetype==0?'业主车':item.feetype==1?'月费车':item.feetype==2?'临时车':item.feetype==3?'业主转临时':item.feetype==4?'无牌车':'数据错误')+''}}</view></view></block></view><view class="page-footer"><view class="plateimg"><view class="footer-title">车牌图片</view><view class="footer-item"><block wx:if="{{item.pic_min==0||!item.pic_min}}"><image class="cardetailsimg" src="/static/mr.png" mode="width"></image></block><block wx:else><image class="cardetailsimg" src="{{'https://anweier-acc.oss-cn-chengdu.aliyuncs.com/'+usercodes+'/'+'car/scan/min/'+item.pic_min}}" mode="width"></image></block></view></view><view class="vehicleimg"><view class="footer-title">车辆图片</view><view class="footer-item"><block wx:if="{{item.pic_max==0||!item.pic_max}}"><image class="cardetailsimg" src="/static/mr.png" mode="width"></image></block><block wx:else><image class="cardetailsimg" src="{{'https://anweier-acc.oss-cn-chengdu.aliyuncs.com/'+usercodes+'/'+'car/scan/max/'+item.pic_max}}" mode="width"></image></block></view></view></view></view>